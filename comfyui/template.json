{
  "displayName": "ComfyUI + Manager + Jupyter (Persistent)",
  "imageName": "ghcr.io/michael-grunewalder/comfyui:latest",
  "gpuCount": 1,
  "ports": [
    { "port": 8188, "protocol": "HTTP" },
    { "port": 8888, "protocol": "HTTP" }
  ],
  "volumeMounts": [
    {
      "name": "network-storage",
      "mountPath": "/workspace"
    }
  ],
  "env": {
    "START_JUPYTER": "true"
  },
  "entrypoint": ["/start.sh"]
}