{
  "name": "Forge + FLUX + Jupyter",
  "description": "Forge Web UI with FLUX support and integrated JupyterLab. Uses shared model directory for all checkpoints and LoRA files.",
  "image": "ghcr.io/michael-grunewalder/forge:latest",
  "port": 7860,
  "startScript": "/workspace/Apps/Forge/start.sh",
  "volumeMounts": [
    { "containerPath": "/workspace", "hostPath": "/workspace" }
  ],
  "env": {
    "START_JUPYTER": "true"
  },
  "ports": [
    { "containerPort": 7860, "protocol": "http" },
    { "containerPort": 8889, "protocol": "http" }
  ],
  "defaultVolumeSizeGb": 50,
  "defaultGpu": "A6000",
  "categories": ["Stable Diffusion", "FLUX", "Forge"]
}