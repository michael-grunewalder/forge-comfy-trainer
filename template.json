{
  "name": "Bearny#s AI Lab",
  "image_name": "ghcr.io/michael-grunewalder/bearnys-ai-lab:latest",
  "containerDiskGB": 20,
  "volumeInGB": 50,
  "volumeMountPath": "/workspace",
  "ports": [
    { "containerPort": 7860, "hostPort": 7860, "protocol": "tcp", "httpProxy": true, "path": "/" },
    { "containerPort": 8188, "hostPort": 8188, "protocol": "tcp", "httpProxy": true, "path": "/" },
    { "containerPort": 8888, "hostPort": 8888, "protocol": "tcp", "httpProxy": true, "path": "/" }
  ],
  "env": [
    { "key": "PYTHONUNBUFFERED", "value": "1" }
  ],
  "gpuTypeId": "NVIDIA",
  "startCommand": "/opt/start.sh"
}