{
  "id": "forge-comfy-trainer-lab",
  "name": "Forge + ComfyUI + Trainer + JupyterLab",
  "description": "Stable Diffusion stack with Forge, ComfyUI, kohya_ss, and JupyterLab. All apps share /workspace/shared/{models,outputs,notebooks,logs}.",
  "image_name": "ghcr.io/michael-grunewalder/forge-comfy-trainer:latest",
  "default_volume_path": "/workspace",
  "volume_mount_path": "/workspace",
  "gpu_type": "NVIDIA",
  "ports": [
    { "container_port": 7860, "host_port": 7860, "protocol": "http", "description": "Forge UI" },
    { "container_port": 8188, "host_port": 8188, "protocol": "http", "description": "ComfyUI" },
    { "container_port": 8888, "host_port": 8888, "protocol": "http", "description": "JupyterLab" }
  ],
  "start_script": "/opt/start.sh",
  "tags": ["stable-diffusion", "training", "comfyui", "forge", "kohya", "jupyter"]
}